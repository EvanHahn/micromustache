{
  "name": "micromustache",
  "version": "3.1.0",
  "description": "A faster and smaller subimplementation of the {{mustache}} template engine for JavaScript",
  "keywords": [
    "template",
    "mustache",
    "string processing",
    "string interpolation",
    "template engine",
    "microlibrary"
  ],
  "main": "src/micromustache.js",
  "browser": "browser/micromustache.min.js",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "mustache": "^2.2.1",
    "uglify-js": "^2.7.0",
    "umd": "^3.0.1"
  },
  "scripts": {
    "build:umd": "umd micromustache src/micromustache.js browser/micromustache.js --commonJS",
    "build:min": "uglifyjs browser/micromustache.js -o browser/micromustache.min.js --compress --mangle",
    "build": "npm run build:umd && npm run build:min",
    "test": "mocha test/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/userpixel/micromustache.git"
  },
  "author": "Alex Ewerl√∂f",
  "license": "MIT"
}
